{
  "name": "@edu-tosel/utils",
  "version": "0.1.0",
  "description": "TOSEL Typescript utils",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json --noEmit false",
    "build-publish": "npm run build && node setupPackage.js && cd dist && npm publish",
